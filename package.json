  "bugs": {
    "url": "https://github.com/seigler/Dash-html-ticker/issues"
  },
  "homepage": "https://github.com/seigler/Dash-html-ticker#readme",
  "devDependencies": {
    "inline-assets": "^1.0.0",
    "jshint": "^2.9.2"
  }
}
